{
  "manifest_version": 3,
  "name": "LD-Notion 书签桥接",
  "version": "1.0.0",
  "description": "为 LD-Notion 用户脚本提供浏览器书签读取能力",
  "permissions": ["bookmarks"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
